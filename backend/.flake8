[flake8]
max-line-length = 120
extend-ignore = E203, E266, E501, W503
exclude =
    .git,
    __pycache__,
    venv,
    env,
    .venv,
    migrations,
    node_modules,
    build,
    dist,
    *.egg-info
max-complexity = 15
per-file-ignores =
    __init__.py:F401
    tests/*:D100,D101,D102,D103

